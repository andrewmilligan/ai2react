{
  "name": "ai2react",
  "version": "1.0.0",
  "description": "Code for the ai2react library",
  "license": "ISC",
  "repository": "git@gitlab.inside.ap.org:newsapps/ai2react",
  "publishConfig": {
    "registry": "https://artifactory.ap.org/api/npm/npm/"
  },
  "scripts": {
    "release": "echo 'Successfully released package!'",
    "prerelease": "yarn version",
    "postversion": "yarn build && git push --tags && yarn publish ./dist/ --new-version $npm_package_version && git push",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf ./dist/",
    "build": "rollup -c",
    "lint": "eslint \"src/**/*.js\"",
    "storybook": "start-storybook -s ./.storybook/public -p 6006",
    "build-storybook": "build-storybook",
    "postbuild-storybook": "cp -r ./.storybook/public/data storybook-static/"
  },
  "main": "index.js",
  "module": "index.esm.js",
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@associatedpress/eslint-config-interact": "^1.0.3",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-docs": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-viewport": "^6.3.4",
    "@storybook/addons": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "babel-loader": "^8.2.2",
    "eslint": "^7.30.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.53.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  }
}
